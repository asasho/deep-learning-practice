   "source": [
    "import numpy as np\n",
    "\n",
    "def binary_cross_entropy(y, t):\n",
    "    # E = -1 * (t * log(y) + (1 - t) * log(1 - y))\n",
    "    loss = -1 * (t * np.log(y) + (1 - t) * np.log(1 - y))\n",
    "    return loss\n",
    "\n",
    "t = [0, 1]\n",
    "y = [0.3, 0.7]\n",
    "\n",
    "x = binary_cross_entropy(np.array(y), np.array(t))\n",
    "print(x)"
   ]
  }
