    "import numpy as np\n",
    "\n",
    "def mean_squared_error(y, t):\n",
    "    # E = 1/2 * Î£ (y - t)^2\n",
    "    return 0.5 * np.sum((y - t) **2)\n",
    "\n",
    "t = [0, 0, 1]\n",
    "y = [0.1, 0.2, 0.7]\n",
    "\n",
    "x = mean_squared_error(np.array(y), np.array(t))\n",
    "\n",
    "print(x)"
   ]
